                      li
                        a(href= cfg.nav.gallery.submenu.galleryCobbles.href)= cfg.nav.gallery.submenu.galleryCobbles.title
                  li(class=(navbar.active == 9 ? 'active' : ''))
                    a(href= cfg.nav.contacts.href)= cfg.nav.contacts.title