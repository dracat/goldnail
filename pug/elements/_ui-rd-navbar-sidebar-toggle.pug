                        li
                          a(href= cfg.nav.pages.submenu.tabsAccordions.href)= cfg.nav.pages.submenu.tabsAccordions.title
                        li
                          a(href= cfg.nav.pages.submenu.progressBar.href)= cfg.nav.pages.submenu.progressBar.title
                        li
                          a(href= cfg.nav.pages.submenu.icons.href)= cfg.nav.pages.submenu.icons.title
                        li
                          a(href= cfg.nav.pages.submenu.grid.href)= cfg.nav.pages.submenu.grid.title
                      ul.offset-md-top-5
                        li
                          a(href= cfg.nav.pages.submenu.headerCorporate.href)= cfg.nav.pages.submenu.headerCorporate.title
                        li
                          a(href= cfg.nav.pages.submenu.headerCentreFooterCentreDark.href)= cfg.nav.pages.submenu.headerCentreFooterCentreDark.title
                        li
                          a(href= cfg.nav.pages.submenu.headerMinimalFooterMinimalDark.href)= cfg.nav.pages.submenu.headerMinimalFooterMinimalDark.title
                        li
                          a(href= cfg.nav.pages.submenu.headerTransparentFooterCentreLight.href)= cfg.nav.pages.submenu.headerTransparentFooterCentreLight.title
                        li
                          a(href= cfg.nav.pages.submenu.headerHamburgerMenu.href)= cfg.nav.pages.submenu.headerHamburgerMenu.title
                        li
                          a(href= cfg.nav.pages.submenu.footerWidgetDark.href)= cfg.nav.pages.submenu.footerWidgetDark.title
                        li
                          a(href= cfg.nav.pages.submenu.footerWidgetLight.href)= cfg.nav.pages.submenu.footerWidgetLight.title
                        li
                          a(href= cfg.nav.pages.submenu.footerMinimalLight.href)= cfg.nav.pages.submenu.footerMinimalLight.title
                  li(class=(navbar.active == 8 ? 'active' : '')).rd-navbar--has-dropdown.rd-navbar-submenu
                    a(href= cfg.nav.gallery.href)= cfg.nav.gallery.title
                    ul.rd-navbar-dropdown
                      li
                        a(href= cfg.nav.gallery.submenu.galleryGrid.href)= cfg.nav.gallery.submenu.galleryGrid.title
                      li
                        a(href= cfg.nav.gallery.submenu.gridWithoutPaddingGallery.href)= cfg.nav.gallery.submenu.gridWithoutPaddingGallery.title
                      li
