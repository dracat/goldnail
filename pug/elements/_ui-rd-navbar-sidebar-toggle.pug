                        li
                          a(href= cfg.nav.pages.submenu.singleDeal.href)= cfg.nav.pages.submenu.singleDeal.title
                      ul.offset-md-top-5
                        li
                          a(href= cfg.nav.pages.submenu.typography.href)= cfg.nav.pages.submenu.typography.title
                        li
                          a(href= cfg.nav.pages.submenu.buttons.href)= cfg.nav.pages.submenu.buttons.title
                        li
